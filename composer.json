{
    "name": "askonaweb/elastic-query-builder",
    "description": "Query Builder for Elasticsearch v7",
    "homepage": "https://github.com/AskonaWeb/elastic-query-builder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Leppik",
            "email": "n.leppik@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AskonaWeb\\ElasticQueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AskonaWeb\\ElasticQueryBuilder\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^5.6",
        "symfony/var-dumper": "^5.4"
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
